{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Store</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Sn</th>
                <td>{{ entity.sn }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ entity.phone }}</td>
            </tr>
            <tr>
                <th>Openregion</th>
                <td>{{ entity.openRegion }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ entity.mail }}</td>
            </tr>
            <tr>
                <th>Isshow</th>
                <td>{{ entity.isShow }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_store') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_store_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
   {#  <li>{{ form(delete_form) }}</li> #}
</ul>
{% endblock %}
