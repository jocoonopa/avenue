{% for key, product in GoodsPassports %}
	<h3 data-page="{{ nCount|default(1) }}" data-nowPage="{{ nNowPage|default(1) }}" data-imgpath="{% if product.img %}{{product.img.path }}{% endif %}">
		<em>{% if product.img %}{{ product.img.id }}{% endif %}</em>
		<p class="p__product-sn" data-id="{{ product.id }}">{{ product.sn }}</p>
	</h3>
	<div>
		<div class="accordion_left">
			<div id="ResTabs{{ product.id }}" class="goodsResTabs">
				{% include 'WoojinGoodsBundle:Goods/Detail:tabTitle.html.twig' with {product: product} %}

				{% include 'WoojinGoodsBundle:Goods/Detail:tabContent.html.twig' with {product: product} %}
			</div>
		</div>
		<div class="accordion_right"></div>
	</div>
{% endfor %}